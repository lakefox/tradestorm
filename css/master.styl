html, body
  margin: 0
  padding: 0
  background: #01005e

.title
  font-family: 'Staatliches'
  font-size: 40px
  color: #17b794
  margin: 20px

.center_col
  width: 90%
  max-width: 700px
  margin: auto

.box
  background: #28518a
  width: 100%
  padding-bottom: 50px
  margin-bottom: 50px

.head
  background: #17b794
  color: #fff
  width: calc(100% - 20px)
  height: 80px
  font-family: 'Oswald'
  font-size: 27px
  line-height: 80px
  padding-left: 20px

.price
  float: right
  padding-right: 20px
  font-size: 30px

.content
  display: flex

.col
  width: 50%
  height: 100%
  margin-top: 50px;

.col_head
  color: #fff
  font-family: 'Oswald'
  text-align: center
  font-size: 25px

.buy_price
  text-align: center
  font-size: 45px
  color: #fff
  font-family: 'Oswald'
  margin-top: 15px

.choice
  text-align: center
  font-size: 40px
  font-family: 'Oswald'
  color: #fff
  margin-top: 10px

#edit
  color: #fff
  float: right
